<?xml version="1.0" encoding="utf-8"?>
<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>
    <!-- Disable face detection on the front camera -->
    <bool name="noFaceDetectOnFrontCamera">true</bool>
    <!-- Disable face detection on the rear camera -->
    <bool name="noFaceDetectOnRearCamera">true</bool>
    <!-- Use ZSL mode -->
    <bool name="enableZSL">false</bool>
</resources>

