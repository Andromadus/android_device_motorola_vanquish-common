# Android fstab file.
#<src>                          <mnt_point>         <type>    <mnt_flags and options>                                                          <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
loop@/ss/safestrap/rom-slot1/system.img    /system             ext4      rw,noatime,nodiratime,noauto_da_alloc,barrier=1                 wait
loop@/ss/safestrap/rom-slot1/userdata.img  /data               ext4      rw,nosuid,nodev,noatime,nodiratime,noauto_da_alloc,barrier=1    wait,check,encryptable=footer
loop@/ss/safestrap/rom-slot1/cache.img     /cache              ext4      rw,nosuid,nodev,noatime,nodiratime,noauto_da_alloc,barrier=1    wait,check
/dev/block/platform/msm_sdcc.1/by-name/modem     /firmware           ext4      ro,nosuid,nodev,noatime,nodiratime,barrier=1                    wait,check
/dev/block/platform/msm_sdcc.1/by-name/persist   /persist            ext4      ro,nosuid,nodev,noatime,nodiratime,barrier=1                    wait,check
